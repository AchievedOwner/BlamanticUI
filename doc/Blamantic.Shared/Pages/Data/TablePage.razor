<PresentationPart Title="Demo">
    <Description>
        <Message State="State.Positive">
            Tables will automatically stack their layouts for mobile devices. To disable this behavior, use the unstackable variation or tablet stackable to allow responsive adjustments for tablet.
        </Message>
    </Description>
    <RunTemplate>
        <Table Celled>
            <Header>
                <TableRow>
                    <TableCell Header>
                        Name
                    </TableCell>
                    <TableCell Header>
                        Age
                    </TableCell>
                    <TableCell Header>
                        Job
                    </TableCell>
                </TableRow>
            </Header>
            <Body>
                <TableRow>
                    <TableCell>
                        John
                    </TableCell>
                    <TableCell>
                        25
                    </TableCell>
                    <TableCell>
                        Engineer
                    </TableCell>
                </TableRow>
                <TableRow>
                    <TableCell>
                        Bill
                    </TableCell>
                    <TableCell>
                        40
                    </TableCell>
                    <TableCell>
                        Engineer
                    </TableCell>
                </TableRow>
                <TableRow>
                    <TableCell>
                        Elvis
                    </TableCell>
                    <TableCell>
                        32
                    </TableCell>
                    <TableCell>
                        Designer
                    </TableCell>
                </TableRow>
            </Body>
            <Footer>
                <TableRow>
                    <TableCell colspan="3" HorizontalAlignment="HorizontalAlignment.Right">Total 3 </TableCell>
                </TableRow>
            </Footer>
        </Table>
    </RunTemplate>
