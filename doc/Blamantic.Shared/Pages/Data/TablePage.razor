<PresentationPart Title="高亮状态">
    <Description>
        设置 <code>State</code> 参数配置整行或某一个单元格的高亮醒目的颜色。
    </Description>
    <RunTemplate>
        <Table Celled="true">
            <Header>
                <Tr>
                    <Th>名称</Th>
                    <Th>状态</Th>
                    <Th>结果</Th>
                </Tr>
            </Header>
            <Body>
                <Tr>
                    <Td>张三</Td>
                    <Td>未知</Td>
                    <Td State="State.Negative">错误</Td>
                </Tr>
                <Tr State="State.Positive">
                    <Td>李四</Td>
                    <Td>通过</Td>
                    <Td><Icon IconClass="checkmark" /></Td>
                </Tr>
                <Tr State="State.Warning">
                    <Td>王五</Td>
                    <Td><Icon IconClass="attention" /> 需补充信息</Td>
                    <Td></Td>
                </Tr>
                <Tr State="State.Negative">
                    <Td>赵六</Td>
                    <Td>驳回</Td>
                    <Td><Icon IconClass="close" /> 查看详情</Td>
                </Tr>
                <Tr>
                    <Td>孙七</Td>
                    <Td>未知</Td>
                    <Td State="State.Warning"><Icon IconClass="attention" /> 缺少签名</Td>
                </Tr>
                <Tr State="State.Error">
                    <Td>李八</Td>
                    <Td>错误</Td>
                    <Td><Icon IconClass="close" /> 数据错误</Td>
                </Tr>
            </Body>
        </Table>
    </RunTemplate>
