<PresentationPart Title="State">
    <Description>
        A cell or row may let a user know whether a value is good or bad
    </Description>
    <RunTemplate>
        <Table Celled>
            <Header>
                <Tr>
                    <Th>Name</Th>
                    <Th>State</Th>
                    <Th>Result</Th>
                </Tr>
            </Header>
            <Body>
                <Tr>
                    <Td>Jimmy</Td>
                    <Td>Unknown</Td>
                    <Td State="State.Negative">Error</Td>
                </Tr>
                <Tr State="State.Positive">
                    <Td>Bill</Td>
                    <Td>Passed</Td>
                    <Td><Icon IconClass="checkmark" /></Td>
                </Tr>
                <Tr State="State.Warning">
                    <Td>Black</Td>
                    <Td><Icon IconClass="attention" /> Need complete</Td>
                    <Td></Td>
                </Tr>
                <Tr State="State.Negative">
                    <Td>Phill</Td>
                    <Td>Rejected</Td>
                    <Td><Icon IconClass="close" /> Detail</Td>
                </Tr>
                <Tr>
                    <Td>Sara</Td>
                    <Td>Unknown</Td>
                    <Td State="State.Warning"><Icon IconClass="attention" /> Missing Signature</Td>
                </Tr>
                <Tr State="State.Error">
                    <Td>Geoge</Td>
                    <Td>Error</Td>
                    <Td><Icon IconClass="close" /> Data Error</Td>
                </Tr>
            </Body>
        </Table>
    </RunTemplate>
