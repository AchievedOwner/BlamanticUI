<PresentationPart Title="State">
    <Description>
        A cell or row may let a user know whether a value is good or bad
    </Description>
    <RunTemplate>
        <Table Celled>
            <Header>
                <TableRow>
                    <TableCell Header>Name</TableCell>
                    <TableCell Header>State</TableCell>
                    <TableCell Header>Result</TableCell>
                </TableRow>
            </Header>
            <Body>
                <TableRow>
                    <TableCell>Jimmy</TableCell>
                    <TableCell>Unknown</TableCell>
                    <TableCell State="State.Negative">Error</TableCell>
                </TableRow>
                <TableRow State="State.Positive">
                    <TableCell>Bill</TableCell>
                    <TableCell>Passed</TableCell>
                    <TableCell><Icon IconClass="checkmark" /></TableCell>
                </TableRow>
                <TableRow State="State.Warning">
                    <TableCell>Black</TableCell>
                    <TableCell><Icon IconClass="attention" /> Need complete</TableCell>
                    <TableCell></TableCell>
                </TableRow>
                <TableRow State="State.Negative">
                    <TableCell>Phill</TableCell>
                    <TableCell>Rejected</TableCell>
                    <TableCell><Icon IconClass="close" /> Detail</TableCell>
                </TableRow>
                <TableRow>
                    <TableCell>Sara</TableCell>
                    <TableCell>Unknown</TableCell>
                    <TableCell State="State.Warning"><Icon IconClass="attention" /> Missing Signature</TableCell>
                </TableRow>
                <TableRow State="State.Error">
                    <TableCell>Geoge</TableCell>
                    <TableCell>Error</TableCell>
                    <TableCell><Icon IconClass="close" /> Data Error</TableCell>
                </TableRow>
            </Body>
        </Table>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<Table Celled>
