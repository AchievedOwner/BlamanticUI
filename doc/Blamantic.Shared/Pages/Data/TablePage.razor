<PresentationPart Title="Demo">
    <Description>
        <Message State="State.Positive">
            Tables will automatically stack their layouts for mobile devices. To disable this behavior, use the unstackable variation or tablet stackable to allow responsive adjustments for tablet.
        </Message>
    </Description>
    <RunTemplate>
        <Table Celled>
            <Header>
                <Tr>
                    <Th>
                        Name
                    </Th>
                    <Th>
                        Age
                    </Th>
                    <Th>
                        Job
                    </Th>
                </Tr>
            </Header>
            <Body>
                <Tr>
                    <Td>
                        John
                    </Td>
                    <Td>
                        25
                    </Td>
                    <Td>
                        Engineer
                    </Td>
                </Tr>
                <Tr>
                    <Td>
                        Bill
                    </Td>
                    <Td>
                        40
                    </Td>
                    <Td>
                        Engineer
                    </Td>
                </Tr>
                <Tr>
                    <Td>
                        Elvis
                    </Td>
                    <Td>
                        32
                    </Td>
                    <Td>
                        Designer
                    </Td>
