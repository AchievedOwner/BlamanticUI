@page "/component/common/label"

<Pager DataSource="Menu">
    <Body>
<Header1 Divider="true">
    标签
    <SubHeader>
        标签表明内容的类别
    </SubHeader>
</Header1>

<Message>
    使用 <code>Label</code> 组件表示标签。
</Message>

<PresentationPart Title="示例">
    <RunTemplate>
        <Label><Icon IconClass="mail" /> 123</Label>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<Label><Icon IconClass=""mail""/> 123</Label>
```
        ")
    </CodeTemplate>
</PresentationPart>

<PresentationPart Title="颜色">
    <Description>
        设置 <code>Color</code> 可让标签具备颜色。
    </Description>
    <RunTemplate>
        <Label Color="Color.Red"><Icon IconClass="mail" /> 99+</Label>
        <Label Color="Color.Grey"><Icon IconClass="plus" /> 加关注</Label>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<Label Color=""Color.Red""><Icon IconClass=""mail"" /> 99+</Label>
<Label Color=""Color.Grey""><Icon IconClass=""plus"" /> 加关注</Label>
```
        ")
    </CodeTemplate>
</PresentationPart>

<PresentationPart Title="超链接">
    <Description>
        使用 <code>LabelAnchor</code>  即可让超链接呈现标签样式。
    </Description>
    <RunTemplate>
        <LabelAnchor Link="/">
            <Icon IconClass="plus" /> 关注
        </LabelAnchor>
    </RunTemplate>
    <CodeTemplate>
@Code.GetCode(@"
```html
<LabelAnchor Link=""/"">
    <Icon IconClass=""plus"" /> 关注
</LabelAnchor>
```
")
    </CodeTemplate>
</PresentationPart>

<PresentationPart Title="图像">
    <Description>
        设置 <code>Label</code> 组件 <code>Image="true"</code> 然后包含 <code>Image</code> 组件呈现图像。
    </Description>
    <RunTemplate>
        <Label Image="true" Color="Color.Red">
            <Image Source="img/avatar/ade.jpg"  /> Ade
        </Label>
        <Label Image="true" Color="Color.Green">
            <Image Source="img/avatar/christian.jpg"  /> Christian
        </Label>
        <Label Image="true" Color="Color.Brown">
            <Image Source="img/avatar/elliot.jpg"  /> Elliot
        </Label>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<Label Image=""true"" Color=""Color.Red"">
    <Image Source=""img/avatar/ade.jpg"" /> Ade
</Label>
<Label Image=""true"" Color=""Color.Green"">
    <Image Source=""img/avatar/christian.jpg"" /> Christian
</Label>
<Label Image=""true"" Color=""Color.Brown"">
    <Image Source=""img/avatar/elliot.jpg"" /> Elliot
</Label>
```
")
    </CodeTemplate>
</PresentationPart>
<PresentationPart Title="超链接图像">
    <RunTemplate>
        <LabelAnchor Image="true"  Link="#">
            <Image Source="img/avatar/jenny.jpg"  /> Jenny
        </LabelAnchor>
        <LabelAnchor Image="true" Link="#">
            <Image Source="img/avatar/joe.jpg"  /> Joe
        </LabelAnchor>
        <LabelAnchor Image="true" Link="#">
            <Image Source="img/avatar/nan.jpg"  /> Nan
        </LabelAnchor>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<LabelAnchor Image=""true"" Link=""#"">
    <Image Source=""img/avatar/jenny.jpg"" /> Jenny
</LabelAnchor>
<LabelAnchor Image=""true"" Link=""#"">
    <Image Source=""img/avatar/joe.jpg"" /> Joe
</LabelAnchor>
<LabelAnchor Image=""true"" Link=""#"">
    <Image Source=""img/avatar/nan.jpg"" /> Nan
</LabelAnchor>
```
")
    </CodeTemplate>
</PresentationPart>

<PresentationPart Title="带详情">
    <Description>
        在 <code>Label</code> 组件中添加 <code>LabelDetail</code> 组件。
    </Description>
    <RunTemplate>
        <Label Image="true" Color="Color.Orange">
            <Image Source="img/avatar/stevie.jpg"  /> Stevie
            <LabelDetail>软件工程师</LabelDetail>
        </Label>
        <Label Image="true" Color="Color.Yellow">
            <Image Source="img/avatar/veronika.jpg"  /> Veronika
            <LabelDetail>UI 设计师</LabelDetail>
        </Label>
        <Label Image="true" Color="Color.Purple">
            <Image Source="img/avatar/zoe.jpg"  /> Zoe
            <LabelDetail>项目经理</LabelDetail>
        </Label>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<Label Image=""true"" Color=""Color.Orange"">
    <Image Source=""img/avatar/stevie.jpg"" /> Stevie
    <LabelDetail>软件工程师</LabelDetail>
</Label>
<Label Image=""true"" Color=""Color.Yellow"">
    <Image Source=""img/avatar/veronika.jpg"" /> Veronika
    <LabelDetail>UI 设计师</LabelDetail>
</Label>
<Label Image=""true"" Color=""Color.Purple"">
    <Image Source=""img/avatar/zoe.jpg"" /> Zoe
    <LabelDetail>项目经理</LabelDetail>
</Label>
```
")
    </CodeTemplate>
</PresentationPart>
