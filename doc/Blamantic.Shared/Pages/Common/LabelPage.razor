<PresentationPart Title="吸附">
    <Description>
        设置 <code>Attached="true"</code> 以及方位可以让标签吸附在容器中。
    </Description>
    <RunTemplate>
        <Grid ColSpan="ColSpan.Three3">
            <Column>
                <Segment>
                    <Label Attached="true" AttachedVertical="VerticalPosition.Top">HTML</Label>
                    <ParagraphDemo  FullWidth="true"/>
                </Segment>
            </Column>
            <Column>
                <Segment>
                    <Label Attached="true" AttachedVertical="VerticalPosition.Bottom">CSS</Label>
                    <ParagraphDemo  FullWidth="true"/>
                </Segment>
            </Column>
            <Column>
                <Segment>
                    <Label Attached="true" AttachedHorizontal="HorizontalPosition.Right" AttachedVertical="VerticalPosition.Top">CODE</Label>
                    <ParagraphDemo  FullWidth="true"/>
                </Segment>
            </Column>
            <Column>
                <Segment>
                    <Label Attached="true" AttachedVertical="VerticalPosition.Top" AttachedHorizontal="HorizontalPosition.Left">代码评审</Label>
                    <ParagraphDemo  FullWidth="true"/>
                </Segment>
            </Column>
            <Column>
                <Segment>
                    <Label Attached="true" AttachedHorizontal="HorizontalPosition.Left" AttachedVertical="VerticalPosition.Bottom">用户反馈</Label>
                    <ParagraphDemo  FullWidth="true"/>
                </Segment>
            </Column>
            <Column>
                <Segment>
                    <Label Attached="true" AttachedHorizontal="HorizontalPosition.Right" AttachedVertical="VerticalPosition.Bottom">Script</Label>
                    <ParagraphDemo  FullWidth="true"/>
                </Segment>
            </Column>
        </Grid>
    </RunTemplate>
    <CodeTemplate>
        @Code.GetCode(@"
```html
<Grid ColSpan=""ColSpan.Three3"">
    <Column>
        <Segment>
            <Label Attached=""true"" AttachedVertical=""VerticalPosition.Top"">HTML</Label>
            <ParagraphDemo />
        </Segment>
    </Column>
    <Column>
        <Segment>
            <Label Attached=""true"" AttachedVertical=""VerticalPosition.Bottom"">CSS</Label>
            <ParagraphDemo />
        </Segment>
    </Column>
    <Column>
        <Segment>
            <Label Attached=""true"" AttachedHorizontal=""HorizontalPosition.Right"" AttachedVertical=""VerticalPosition.Top"">CODE</Label>
            <ParagraphDemo />
        </Segment>
    </Column>
    <Column>
        <Segment>
            <Label Attached=""true"" AttachedVertical=""VerticalPosition.Top"" AttachedHorizontal=""HorizontalPosition.Left"">代码评审</Label>
            <ParagraphDemo />
        </Segment>
    </Column>
    <Column>
        <Segment>
            <Label Attached=""true"" AttachedHorizontal=""HorizontalPosition.Left"" AttachedVertical=""VerticalPosition.Bottom"">用户反馈</Label>
            <ParagraphDemo />
        </Segment>
    </Column>
    <Column>
        <Segment>
            <Label Attached=""true"" AttachedHorizontal=""HorizontalPosition.Right"" AttachedVertical=""VerticalPosition.Bottom"">Script</Label>
            <ParagraphDemo />
        </Segment>
    </Column>
</Grid>
```
")
    </CodeTemplate>
</PresentationPart>
